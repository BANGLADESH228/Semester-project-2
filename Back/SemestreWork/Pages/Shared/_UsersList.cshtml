@using SemestreWork.Models;
@model List<RegisterModel>

    @foreach (var Users in Model)
    {
    <div class="timeline-header">
        <span class="userimage">
            @if (@Users.Image == null)
            {
                <img src="~/img/noavatar.png">
            }
            else
            {
                <img src="@Users.Image">
            }
        </span>
        <span class="username">
            <a href="/Profile/@Users.Id">@Users.Name</a> <!-- Ну тут никнейм (псевдоним) от айдишника -->
            <small></small>
        </span>
    </div>
    }


